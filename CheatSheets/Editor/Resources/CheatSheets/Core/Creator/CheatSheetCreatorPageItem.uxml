<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements">
    <ui:Foldout text="[Placeholder]" name="Foldout" style="padding-left: 3px; padding-right: 3px; padding-top: 3px; padding-bottom: 3px; background-color: rgb(58, 58, 58); margin-left: 2px; margin-right: 2px; margin-top: 2px; margin-bottom: 2px; border-top-left-radius: 10px; border-bottom-left-radius: 10px; border-top-right-radius: 10px; border-bottom-right-radius: 10px;">
        <ui:TextField picking-mode="Ignore" label="Name" name="PageName" tooltip="Should describe the main content of this page. Can be empty. Is used by the search engine." />
        <ui:Toggle label="Use Number" name="UseNumber" tabindex="0" tooltip="Will use this page number. Can be useful if you want this specific page to have no number." />
        <uie:IntegerField label="Number" value="0" readonly="false" tooltip="Must be superior or equal to 1 and unique in its section. 0 means no page, so its an invalid number." name="PageNumber" />
        <ui:Toggle label="Show Page Number" tooltip="Will not show the page number in the selector window." name="ShowPageNumber" style="margin-left: 3px; margin-top: 3px;" />
        <ui:TextField picking-mode="Ignore" label="Keywords" value="filler text" name="Keywords" tooltip="Should describe the content of this page. Is used by the search engine. Can be empty. Must use commas as a separator between words." />
